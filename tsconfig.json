{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "allowJs": true,
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "jsx": "react"
  },
  "include": [
    "js/**/*.ts",
    "js/**/*.tsx",
    "js/**/*.js",
    "**/*.d.ts"
  ]
}